<script type="text/javascript">
    $(document).ready(function(){
        $('#sm_slider').smSlider();

    });

    $(window).scroll(function() {
        var nav = $('.smenu');

        if ($(this).scrollTop() > 180) {
            deg = nav.rotationDegrees();
            if (deg <=0){
                deg = deg+2.5;
                if(deg > 0) {
                    deg = 0;
                }
                var srotate = "rotate(" + deg + "deg)";
                nav.css({"-moz-transform" : srotate, "-webkit-transform" : srotate, "-o-transform": srotate, "-ms-transform": srotate, "transform": srotate});
            }
            nav.addClass('fixed');
        }else{
            deg = nav.rotationDegrees();
            if($(this).scrollTop() == 0){
                deg = -11;
            }
            if (deg >= -11){
                deg = deg-2.5;
                if(deg < -11) {
                    deg = -11;
                }
                var srotate = "rotate(" + deg + "deg)";
                nav.css({"-moz-transform" : srotate, "-webkit-transform" : srotate, "-o-transform": srotate, "-ms-transform": srotate, "transform": srotate});
            }
            nav.removeClass('fixed');
        }

    });

    $.fn.rotationDegrees = function () {
         var matrix = this.css("-webkit-transform") ||
    this.css("-moz-transform")    ||
    this.css("-ms-transform")     ||
    this.css("-o-transform")      ||
    this.css("-ms-transform")     ||
    this.css("transform");
    if(typeof matrix === 'string' && matrix !== 'none') {
        var values = matrix.split('(')[1].split(')')[0].split(',');
        var a = values[0];
        var b = values[1];
        var angle = Math.round(Math.atan2(b, a) * (180/Math.PI));
    } else { var angle = 0; }
    return angle;
   };

    </script>
